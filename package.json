{
  "name": "working-time-viewer",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/konojunya/working-time-viewer.git",
  "author": "konojunya <junya03dance@gmail.com>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p . --noEmit",
    "prebuild": "rm -rf dist",
    "build": "webpack-cli --config webpack.config.js",
    "build:watch": "yarn build --watch",
    "postbuild": "cp manifest.json ./dist/manifest.json",
    "jest": "jest",
    "test": "BABEL_ENV=test yarn jest -i",
    "test:watch": "yarn jest --watch",
    "test:approve": "yarn jest -u",
    "test:coverage": "yarn jest --coverage",
    "stats": "yarn --silent build --profile --json > ./stats.json",
    "analyze": "webpack-bundle-analyzer ./stats.json ./ -s gzip"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.86",
    "@types/jest": "24.0.13",
    "@wasm-tool/wasm-pack-plugin": "^0.4.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.8.0",
    "source-map-loader": "0.2.4",
    "terser-webpack-plugin": "1.3.0",
    "ts-loader": "^6.0.2",
    "typescript": "3.5.1",
    "webpack": "4.33.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "worker-plugin": "^3.1.0"
  },
  "dependencies": {
    "preact": "^8.4.2"
  }
}
